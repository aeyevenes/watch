<div class="banner">
  <div class="container">
    <h1>Create and customize your movie lists!</h1>
    <%= link_to 'Create a movie list', new_list_path, class: 'btn btn-primary' %>
  </div>
</div>
<div class="cards">
  <% @lists.each do |list| %>
    <div class="card-category">
      <%= link_to list_path(list), class:'text-decoration-none' do %>
          <%= list.name %>
      <% end %>
    </div>
  <% end %>
</div>
